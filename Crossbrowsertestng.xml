<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Cross browser testing Suite">
<listeners>
<listener class-name="Listeners.Transformer"></listener>
<listener class-name="Listeners.ListenerTest"></listener>
</listeners>
  <test name="ChromeBrowser">
   <parameter name="browser"  value="Chrome"/>
    <classes>
			<class name="Tests.Arraytest" />
			<class name="Tests.Queuetest" />
			<class name="Tests.Registertest" />
			<class name="Tests.Logintest" />
			<class name="Tests.Graphtest" />
			<class name="Tests.Hometest" />
			<class name="Tests.Treetest" />
			<class name="Tests.StackTest" />
			<class name="Tests.LinkedListTest" />
			<class name="Tests.DataStructuretest" />
    </classes>
  </test> <!-- Test -->
   <test name="FirefoxeBrowser">
